//----DP Approach #1----
public class Solution {
    public int uniquePaths(int m, int n) {
        if(m<=1||n<=1){
            return 1;
        }
        int[][] map = new int[m][n];  //new a 2d array
        for(int i=0;i<n;i++){
            map[0][i]=1;
        }
        for(int i=0;i<m;i++){
            map[i][0]=1;
        }
        for(int i=1;i<m;i++){  //row major
            for(int j=1;j<n;j++){
                map[i][j]=map[i-1][j]+map[i][j-1];
            }
        }
        return map[m-1][n-1];
    }
}
